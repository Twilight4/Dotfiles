# Set the layout in this tab
layout grid
# Set the working directory for windows in the current tab
cd ~
# Open 4 windows
launch --title zsh zsh
launch zsh
launch zsh
launch zsh

# Create a new tab
new_tab neovim
cd ~
# Set the layout in this tab
layout grid
# Create a window with nvim (don't use aliases)
launch nvim
# Focus the current tab
focus
