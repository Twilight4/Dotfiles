# Set the layout in this tab
layout tall
# Set the working directory for windows in the current tab
cd ~/
# Create a window (title sets the tab title)
launch --title agenda sh -c 'echo "\033[0;33mPress \"C-c C\" open agenda view with: \"C-x a a o\"\033[0m"; emacsclient -nw --eval "(progn (org-agenda) (delete-other-windows))" && exec zsh'
